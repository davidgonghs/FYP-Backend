<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dghs.fyp.backend.mapper.UsageHistoryMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.dghs.fyp.backend.sys.entity.UsageHistory">
        <id column="history_id" property="historyId" />
        <result column="user_id" property="userId" />
        <result column="usage_time" property="usageTime" />
        <result column="sitting_accuracy" property="sittingAccuracy" />
